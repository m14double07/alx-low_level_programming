#include "variadic_functions.h"
#include <stdarg.h>

/**
*print_numbers - Prints numbers given
*@n: Numbers given
*@separator:String to print between numbers
*/
void print_numbers(const char *separator, const unsigned int n, ...)
{
	unsigned int i;
	va_list lidt;

	va_start(list, n);

	for (i =0; i < n; i++)
	{
	if (!separator)
	printf("%d", va_arg);
	}
}

